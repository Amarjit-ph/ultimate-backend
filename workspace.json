{
  "version": 1,
  "projects": {
    "api-admin": {
      "root": "apps/api-admin",
      "sourceRoot": "apps/api-admin/src",
      "projectType": "application",
      "prefix": "api-admin",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api-admin",
            "main": "apps/api-admin/src/main.ts",
            "tsconfig": "apps/api-admin/tsconfig.app.json",
            "assets": ["apps/api-admin/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api-admin/src/environments/environment.ts",
                  "with": "apps/api-admin/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api-admin:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api-admin/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api-admin/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "core": {
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/core/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "common": {
      "root": "libs/common",
      "sourceRoot": "libs/common/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/common/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/common/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/common",
            "tsconfig": "libs/common/tsconfig.lib.json",
            "packageJson": "libs/common/package.json",
            "main": "libs/common/src/index.ts",
            "assets": ["libs/common/*.md"]
          }
        }
      }
    },
    "cloud": {
      "root": "libs/cloud",
      "sourceRoot": "libs/cloud/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/cloud/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/cloud/jest.config.js",
            "passWithNoTests": true,
            "codeCoverage": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/cloud",
            "tsconfig": "libs/cloud/tsconfig.lib.json",
            "packageJson": "libs/cloud/package.json",
            "main": "libs/cloud/src/index.ts",
            "assets": ["libs/cloud/*.md"]
          }
        }
      }
    },
    "grpc": {
      "root": "libs/grpc",
      "sourceRoot": "libs/grpc/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/grpc/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/grpc/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/grpc",
            "tsconfig": "libs/grpc/tsconfig.lib.json",
            "packageJson": "libs/grpc/package.json",
            "main": "libs/grpc/src/index.ts",
            "assets": ["libs/grpc/*.md"]
          }
        }
      }
    },
    "loadbalancer": {
      "root": "libs/loadbalancer",
      "sourceRoot": "libs/loadbalancer/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/loadbalancer/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/loadbalancer/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/loadbalancer",
            "tsconfig": "libs/loadbalancer/tsconfig.lib.json",
            "packageJson": "libs/loadbalancer/package.json",
            "main": "libs/loadbalancer/src/index.ts",
            "assets": ["libs/loadbalancer/*.md"]
          }
        }
      }
    },
    "permissions": {
      "root": "libs/permissions",
      "sourceRoot": "libs/permissions/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/permissions/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/permissions"],
          "options": {
            "jestConfig": "libs/permissions/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/permissions",
            "tsconfig": "libs/cloud/tsconfig.lib.json",
            "packageJson": "libs/permissions/package.json",
            "main": "libs/permissions/src/index.ts",
            "assets": ["libs/permissions/*.md"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "api-admin"
}
