{
  "version": 1,
  "projects": {
    "un-service": {
      "root": "packages/un-service",
      "sourceRoot": "packages/un-service/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/un-service/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/un-service/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/un-service",
            "tsConfig": "packages/un-service/tsconfig.lib.json",
            "packageJson": "packages/un-service/package.json",
            "main": "packages/un-service/src/index.ts",
            "assets": [
              "packages/un-service/*.md",
              {
                "input": "./packages/un-service/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/un-service",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/un-service",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "un-service-e2e": {
      "projectType": "application",
      "root": "e2e/un-service-e2e",
      "sourceRoot": "e2e/un-service-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "un-service:build",
            "npmPackageName": "@ultimate-backend/un-service",
            "pluginOutputPath": "dist/packages/un-service",
            "jestConfig": "e2e/un-service-e2e/jest.config.js"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
